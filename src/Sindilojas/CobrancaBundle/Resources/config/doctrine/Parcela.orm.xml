<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Sindilojas\CobrancaBundle\Entity\Parcela" table="parcela">
    <indexes>
      <index name="FK_parcela_negociacao" columns="id_negociacao"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="numero" type="integer" column="numero" nullable="true"/>
    <field name="valor" type="float" column="valor" precision="10" scale="0" nullable="false"/>
    <field name="vencimento" type="date" column="vencimento" nullable="false"/>
    <field name="pago" type="boolean" column="pago" nullable="false"/>
    <field name="entrada" type="boolean" column="entrada" nullable="false"/>
    <many-to-one field="idNegociacao" target-entity="Negociacao">
      <join-columns>
        <join-column name="id_negociacao" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
