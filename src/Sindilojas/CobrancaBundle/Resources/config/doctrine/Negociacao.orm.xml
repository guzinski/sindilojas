<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Sindilojas\CobrancaBundle\Entity\Negociacao" table="negociacao">
    <indexes>
      <index name="FK_negociacao_divida" columns="id_divida"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="valor" type="float" column="valor" precision="10" scale="0" nullable="true"/>
    <many-to-one field="idDivida" target-entity="Divida">
      <join-columns>
        <join-column name="id_divida" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
